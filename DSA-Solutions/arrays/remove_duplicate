class Solutions(object):
    def removeDuplicates(self, nums):
        i = 0
        while i < len(nums) - 1:
            if nums[i] == nums[i + 1]:
                nums.pop(i + 1)
            else:
                i += 1
        return len(nums)
    

nums = [1, 1, 2]
print(Solutions().removeDuplicates(nums))
print(len(nums))

